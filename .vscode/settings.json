{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "CodeGPT.apiKey": "CodeGPT Plus Beta",
  "CodeGPT.Autocomplete.enabled": true,
  // "i18n-ally.localesPaths": ["src/renderer/src/i18n", "src/renderer/src/i18n/locales"],
  "i18n-ally.localesPaths": ["src/renderer/public/locales"],
  "i18n-ally.sourceLanguage": "ja",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.enabledFrameworks": ["react-i18next"],
  "i18n-ally.displayLanguage": "ja",
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "i18n-ally.namespace": true,
  "i18n-ally.pathMatcher": "{namespace}/{locale}.{ext}",
  "i18n-ally.defaultNamespace": "translation"
}
